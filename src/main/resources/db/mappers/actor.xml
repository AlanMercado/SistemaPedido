<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD iBatis Mapper 3.0 //EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="mx.edu.uaz.SegundoExamenMyBatis.config.mappers">
<select id="todosActores" resultMap="resultMapactor">
	SELECT a.idActor,a.nombre FROM actor a;
</select>

<resultMap type="actor" id="resultMapactor">
	<id property="idActor" column="idActor"/>
	<result property="nombre" column="idActor"/>
</resultMap>
</mapper>